	<modell>
		<typDefinition pid="typ.testPlPrüfungFormal" name="TypTestPlPrüfungFormal">
			<info>
				<kurzinfo>
					Test-Typ zur Prüfung von SWE PL-Prüfung formal.
				</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.testPlPrüfungFormal"/>
		</typDefinition>

		<attributgruppenDefinition pid="atg.testPlPrüfungFormal" name="AttributgrppeTest">
			<info>
				<kurzinfo>nichts</kurzinfo>
			</info>
			<aspekt pid="asp.testEingang" onlineModus="quelleUndSenke"/>
			<aspekt pid="asp.testAusgang" onlineModus="quelleUndSenke"/>
			<attributliste pid="atl.wertMitStatusGanzZahl" name="Attribut1"/>
			<attributliste pid="atl.wertMitStatusKommaZahl" name="Attribut2"/>
		</attributgruppenDefinition>

		<attributlistenDefinition pid="atl.wertMitStatusGanzZahl" name="WertMitStatusGanzZahl">
			<info>
				<kurzinfo>nichts</kurzinfo>
			</info>
			<attribut pid="att.testAttGanzZahl" name="Wert">
				<info>
					<kurzinfo>nichts</kurzinfo>
				</info>
				<default wert="0"/>
			</attribut>
			<attributliste pid="atl.testStatus" name="Status">
				<info>
					<kurzinfo>
						Statusinformationen zum Wert, der sich aus unterschiedlichen
						Zwischenberechnungen ergibt.
					</kurzinfo>
				</info>
			</attributliste>
		</attributlistenDefinition>

		<attributlistenDefinition pid="atl.wertMitStatusKommaZahl" name="WertMitStatusKommaZahl">
			<info>
				<kurzinfo>nichts</kurzinfo>
			</info>
			<attribut pid="att.testAttKommaZahl" name="Wert">
				<default wert="0"/>
			</attribut>
			
			<attributliste pid="atl.testStatus" name="Status">
				<info>
					<kurzinfo>
						Statusinformationen zum Wert, der sich aus
						unterschiedlichen Zwischenberechnungen ergibt.
					</kurzinfo>
				</info>
			</attributliste>
		</attributlistenDefinition>

		<attributlistenDefinition pid="atl.testStatus" name="TestStatus">
			<info>
				<kurzinfo>nichts</kurzinfo>
			</info>			
			<attributliste pid="atl.statusPlausibilitätsPrüfungFormal" name="PlFormal">
				<info>
					<kurzinfo>
						Statusinformationen werden von der
						formalen Plausibilitätsprügung gesetzt.
					</kurzinfo>
				</info>
			</attributliste>
			<attributliste pid="atl.statusMessWertErsetzung" name="MessWertErsetzung">
				<info>
					<kurzinfo>
						Statusinformation werden von
						der Messwertersetzung gesetzt.
					</kurzinfo>
				</info>
			</attributliste>
		</attributlistenDefinition>
		
		<attributDefinition pid="att.testAttGanzZahl" name="TestAttGanzZahl">
			<info>
				<kurzinfo>nichts</kurzinfo>
			</info>
			<ganzzahl>
				<bereich minimum="-1001" maximum="1001" />
			</ganzzahl>
		</attributDefinition>

		<attributDefinition pid="att.testAttKommaZahl" name="TestAttKommaZahl">
			<info>
				<kurzinfo>nichts</kurzinfo>
			</info>
			<kommazahl genauigkeit="doppelt"/>
